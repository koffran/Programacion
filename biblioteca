#include "biblio.h"

float sumar(float num1,float num2)
{
    float resultado;
    resultado = num1 + num2;

    return resultado;
}

 float restar (float num1,float num2)
 {
     float resultado;
     resultado = num1 - num2;

     return resultado;
 }

 float multiplicar (float num1,float num2)
 {
     float resultado;
     resultado = num1 * num2;

     return resultado;
 }
