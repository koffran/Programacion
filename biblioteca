#include "biblio.h"

float sumar(float num1,float num2)
{
    float resultado;
    resultado = num1 + num2;

    return resultado;
}

 float restar (float num1,float num2)
 {
     float resultado;
     resultado = num1 - num2;

     return resultado;
 }

 float multiplicar (float num1,float num2)
 {
     float resultado;
     resultado = num1 * num2;

     return resultado;
 }

float dividir (float num1,float num2)
 {
     float resultado;
     resultado = num1 / num2;

     return resultado;
 }

 int factorial(int n)
 {
     int resultado;
     int i;
     int acumulador=1;

     for (i=n;i>0;i--)
     {
         acumulador = acumulador*i;
     }
 }
